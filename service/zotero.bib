@article{rouault2019,
  title = {Prefrontal Mechanisms Combining Rewards and Beliefs in Human Decision-Making},
  author = {Rouault, Marion and Drugowitsch, Jan and Koechlin, Etienne},
  date = {2019-01-17},
  journaltitle = {Nature Communications},
