  url = {http://link.springer.com/article/10.1007/s11229-019-02194-w?utm_source=researcher_app&utm_medium=referral&utm_campaign=RESR_MRKT_Researcher_inbound},
  abstract = {We may usefully distinguish between one's having fallible knowledge and having a fallibilist stance on some of one's knowledge. A fallibilist stance could include a concessive knowledge-attribution (CKA). But it might also include a questioning knowledge-attribution (QKA). Attending to the idea of a QKA leads to a distinction between what we may call closed knowledge that p and open knowledge that p. All of this moves us beyond Elgin's classic tale of the epistemic capacities of Holmes and of Watson, and towards a way of resolving Kripke's puzzle about dogmatism and knowing.}
}

@book{heyes2018,
