  abstract = {This paper explores and defends the idea that mental properties and their physical bases {$<$}em class="EmphasisTypeItalic"{$>$}jointly{$<$}/em{$>$} cause their physical effects. The paper evaluates the view as an emergentist response to the exclusion problem, comparing it with a competing nonreductive physicalist solution, the {$<$}em class="EmphasisTypeItalic"{$>$}compatibilist{$<$}/em{$>$} solution, and argues that the joint causation view is more defensible than commonly supposed. Specifically, the paper distinguishes two theses of closure, {$<$}em class="EmphasisTypeItalic"{$>$}Strong Closure{$<$}/em{$>$} and {$<$}em class="EmphasisTypeItalic"{$>$}Weak Closure{$<$}/em{$>$}, two causal~exclusion problems, the {$<$}em class="EmphasisTypeItalic"{$>$}overdetermination problem{$<$}/em{$>$} and the {$<$}em class="EmphasisTypeItalic"{$>$}supervenience problem{$<$}/em{$>$}, and argues that emergentists can avoid the overdetermination problem by denying {$<$}em class="EmphasisTypeItalic"{$>$}Strong Closure{$<$}/em{$>$} and respond to the supervenience problem by accepting the joint causation view.}
}

@book{woodward2005,
  title = {Making {{Things Happen}}: {{A Theory}} of {{Causal Explanation}}},
  shorttitle = {Making {{Things Happen}}},
  author = {Woodward, James},
